# Example .env file for SPARC Client
# Copy this file to .env and fill in your actual values

# Pennsieve configuration
SPARC_PENNSIEVE_PROFILE=prod

# SciCrunch API configuration
SPARC_SCICRUNCH_API_KEY=your-scicrunch-api-key-here

# O2SPARC configuration
SPARC_O2SPARC_HOST=https://api.osparc.io
SPARC_O2SPARC_USERNAME=your-username@example.com
SPARC_O2SPARC_PASSWORD=your-password-here